{
  "name": "sanity-starter",
  "version": "1.0.0",
  "private": true,
  "description": "A Next.js frontend with Sanity Studio",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "NODE_OPTIONS='-r newrelic' next start",
    "studio": "sanity dev --port 3333",
    "studio:build": "sanity build",
    "studio:deploy": "sanity deploy",
    "dev:with-studio": "concurrently \"npm run dev\" \"npm run studio\""
  },
  "dependencies": {
    "@portabletext/react": "^3.0.11",
    "@sanity/assist": "^4.4.7",
    "@sanity/client": "^6.29.1",
    "@sanity/code-input": "^5.1.3",
    "@sanity/color-input": "^4.0.5",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-kit": "^3.2.12",
    "@sanity/vision": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "groq": "^3.14.0",
    "next": "^14.1.0",
    "next-sanity": "^7.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.1.0",
    "sanity": "^4.1.1",
    "sanity-plugin-media": "^3.0.4",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.4.0",
    "turbo": "^1.12.4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "sanity",
    "nextjs",
    "typescript",
    "monorepo",
    "cms",
    "llm"
  ],
  "author": "Your Name",
  "license": "MIT"
}
